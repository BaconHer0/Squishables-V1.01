<Defs>


<!-- Player -->

  <PawnKindDef Name="BasePlayerSquish" Abstract="True">
    <defaultFactionType>PlayerSquish</defaultFactionType>
    <backstoryCategories>
      <li>Civil</li>	  
      <li>Tribal</li>	  
    </backstoryCategories>
    <combatPower>30</combatPower>
    <isFighter>false</isFighter>
    <itemQuality>Good</itemQuality>
    <gearHealthRange>
      <min>1</min>
      <max>1</max>
    </gearHealthRange>	
    <apparelIgnoreSeasons>true</apparelIgnoreSeasons>
    <chemicalAddictionChance>0.06</chemicalAddictionChance>
    <apparelAllowHeadgearChance>0.5</apparelAllowHeadgearChance>
	<apparelTags>
	  <li>IndustrialBasic</li>
      <li>IndustrialAdvanced</li>			
      <li>Outlander</li>
      <li>Neolithic</li>
	</apparelTags>	
    <apparelMoney>
      <min>0</min>
      <max>250</max>
    </apparelMoney>
    <backstoryCryptosleepCommonality>0.05</backstoryCryptosleepCommonality>
    <techHediffsChance>0.03</techHediffsChance>
    <techHediffsMoney>
      <min>0</min>
      <max>800</max>
    </techHediffsMoney>
    <techHediffsTags>
      <li>Poor</li>
      <li>Simple</li>
	  <li>Advanced</li>
    </techHediffsTags>
  </PawnKindDef>

