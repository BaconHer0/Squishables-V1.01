      <height>Middle</height>
      <depth>Outside</depth>	  
      <groups>
        <li>Torso</li>
      </groups>	  

    <parts>
      <li><def>SquishableUndercoat</def>
	  <customLabel>head undercoat</customLabel>	  
	  <coverage>0.1</coverage>
	  <height>Top</height>
	  <groups>
		<li>UpperHead</li>
		<li>FullHead</li>		  
		<li>HeadAttackTool</li>	  
