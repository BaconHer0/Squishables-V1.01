  <RecipeDef ParentName="SurgerySquish">
    <defName>ReplaceStuffingSQ</defName>
    <label>replace lost stuffing</label>
    <description>Replace 'lost stuffing' from age or injury with Cloth or Wool.</description>
    <workerClass>Recipe_RemoveHediff</workerClass>
    <jobString>Replacing stuffing.</jobString>
    <targetsBodyPart>false</targetsBodyPart>	
    <workAmount>3000</workAmount>
    <skillRequirements>
      <Crafting>7</Crafting>
    </skillRequirements>
    <allowMixingIngredients>true</allowMixingIngredients>	
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Cloth</li>
			<li>WoolAlpaca</li>
			<li>WoolCamel</li>
			<li>WoolMuffalo</li>
			<li>WoolMegasloth</li>			
          </thingDefs>
        </filter>
        <count>300</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
		<li>Textiles</li>
      </categories>
      <disallowedThingDefs>
<li>Leather_Patch</li>
<li>Leather_Bear</li>
<li>Leather_Bird</li>
<li>Leather_Bluefur</li>
<li>Leather_Camel</li>
<li>Leather_Chinchilla</li>
<li>Leather_Dog</li>
<li>Leather_Elephant</li>
<li>Leather_Fox</li>
<li>Leather_Heavy</li>
<li>Leather_Human</li>
<li>Leather_Light</li>
<li>Leather_Lizard</li>
<li>Leather_Panthera</li>
<li>Leather_Pig</li>
<li>Leather_Plain</li>
<li>Leather_Rhinoceros</li>
<li>Leather_Thrumbo</li>
<li>Leather_Wolf</li>
      </disallowedThingDefs>
    </fixedIngredientFilter>	
    <!--appliedOnFixedBodyParts>
      <li>SquishableStuffing</li>
    </appliedOnFixedBodyParts-->
    <removesHediff>SoggySorts</removesHediff>
    <successfullyRemovedHediffMessage>{0} has successfully replaced {1}'s stuffing.</successfullyRemovedHediffMessage>
    <deathOnFailedSurgeryChance>0.05</deathOnFailedSurgeryChance>
	</RecipeDef>

